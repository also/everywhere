{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "lib": ["es2019", "dom"],
    "module": "es2020",
    "skipLibCheck": true
  }
}
